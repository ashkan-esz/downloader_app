{
  "expo": {
    "name": "downloader_app",
    "slug": "downloader_app",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "http://localhost:3000/api/manifest?project=downloader_api&channel=default",
      "enabled": true,
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 0,
      "codeSigningCertificate": "./certificate.pem",
      "codeSigningMetadata": {
        "keyid": "main",
        "alg": "rsa-v1_5-sha256"
      }
    },
    "ios": {
      "bundleIdentifier": "com.ashkanaz2828.downloader-app"
    },
    "android": {
      "package": "com.ashkanaz2828.downloader_app"
    },
    "splash": {
      "image": "./src/assets/images/splashScreen.png",
      "backgroundColor": "#22222B",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "assets/images/*"
    ]
  }
}
