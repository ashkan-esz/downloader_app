    "babel-jest": "^29.7.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.7.0",
    "jest-expo": "~50.0.4",
    "metro-source-map": "^0.80.9",
    "react-native-bundle-visualizer": "^3.1.3",
    "react-native-debugger": "^1.1.0",
    "react-test-renderer": "^18.2.0",
    "sharp-cli": "^4.2.0",
    "visualize-bundle": "^1.4.0"
  },
  "engines": {
    "node": ">=18"
  },
  "name": "downloader_app",
  "version": "1.0.0"
