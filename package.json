{
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "expo_start": "expo start",
    "start": "react-native start",
    "test": "jest --verbose --coverage",
    "test:update": "jest --verbose --coverage --updateSnapshot",
    "test:watch": "jest --verbose --watch",
    "coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html",
    "expo_build_android_apk": "expo build:android -t apk ",
    "expo_build_android_bundle": "expo build:android -t app-bundle ",
    "expo_publish": "expo publish --target managed",
    "bundle-visualizer": "react-native-bundle-visualizer --platform android  --expo managed"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./jest-setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "timers": "fake",
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:react-hooks/recommended"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/netinfo": "5.9.7",
    "@react-navigation/bottom-tabs": "^5.11.2",
    "@react-navigation/native": "^5.9.3",
    "@react-navigation/stack": "^5.12.8",
    "axios": "^0.21.1",
    "axios-cache-adapter": "^2.7.3",
    "check-password-strength": "^1.0.15",
    "expo": "^40.0.0",
    "expo-app-loading": "^1.0.2",
    "expo-asset": "~8.2.1",
    "expo-av": "~8.7.0",
    "expo-blur": "~8.2.2",
    "expo-constants": "~9.3.3",
    "expo-font": "~8.4.0",
    "expo-linear-gradient": "~8.4.0",
    "expo-media-library": "~10.0.0",
    "expo-splash-screen": "~0.8.1",
    "expo-status-bar": "~1.0.3",
    "expo-updates": "~0.4.1",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "^6.11.3",
    "react-native": "0.63.4",
    "react-native-collapsible": "^1.5.3",
    "react-native-elements": "^3.0.0-alpha.1",
    "react-native-gesture-handler": "~1.8.0",
    "react-native-ratings": "^7.4.0",
    "react-native-reanimated": "~1.13.0",
    "react-native-safe-area-context": "3.1.9",
    "react-native-screens": "~2.15.2",
    "react-native-swiper": "^1.6.0",
    "react-native-unimodules": "~0.12.0",
    "react-native-vector-icons": "^7.1.0",
    "react-native-web": "~0.13.12",
    "react-query": "^3.12.0",
    "expo-clipboard": "~1.0.1"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@babel/preset-env": "^7.12.1",
    "@testing-library/dom": "^7.28.1",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react-native": "^7.1.0",
    "@testing-library/user-event": "^12.2.2",
    "babel-jest": "^25.2.6",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "~25.2.6",
    "jest-expo": "^40.0.0",
    "react-native-bundle-visualizer": "^2.2.1",
    "react-native-debugger": "^1.1.0",
    "react-test-renderer": "16.14.0",
    "sharp-cli": "^1.14.1"
  }
}
