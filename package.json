{
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "expo_start": "expo start",
    "rc-start": "react-native start --reset-cache",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "build-debug-android": "cd android && ./gradlew assembleDebug",
    "installDebug-android": "cd android && ./gradlew installDebug",
    "build-android": "cd android && ./gradlew assembleRelease",
    "installRelease-android": "cd android && ./gradlew installRelease",
    "bundle-android": "cd android && ./gradlew bundleRelease",
    "expo-build_android_apk": "expo build:android -t apk ",
    "expo-build_android_bundle": "expo build:android -t app-bundle ",
    "expo-publish": "expo publish",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean-start": "npm run clean && npm run rc-start",
    "test": "jest --verbose --coverage",
    "test:update": "jest --verbose --coverage --updateSnapshot",
    "test:watch": "jest --verbose --watch",
    "coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html",
    "bundle-visualizer": "react-native-bundle-visualizer --platform android"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./jest-setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "timers": "fake",
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:react-hooks/recommended"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-community/netinfo": "9.3.7",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "@reduxjs/toolkit": "^1.8.3",
    "@rneui/base": "^4.0.0-rc.5",
    "@rneui/themed": "^4.0.0-rc.5",
    "@shopify/flash-list": "1.4.0",
    "@tanstack/react-query": "^4.0.10",
    "axios": "^0.24.0",
    "expo": "^48.0.21",
    "expo-asset": "~8.9.1",
    "expo-av": "~13.2.1",
    "expo-blur": "~12.2.2",
    "expo-clipboard": "~4.1.2",
    "expo-constants": "~14.2.1",
    "expo-file-system": "~15.2.2",
    "expo-font": "~11.1.1",
    "expo-keep-awake": "~12.0.1",
    "expo-linear-gradient": "~12.1.2",
    "expo-media-library": "~15.2.3",
    "expo-secure-store": "~12.1.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "expo-updates": "~0.16.4",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.33.1",
    "react-native": "0.71.14",
    "react-native-blob-util": "^0.16.2",
    "react-native-device-info": "^10.0.1",
    "react-native-dotenv": "^3.3.0",
    "react-native-drop-shadow": "^0.0.4",
    "react-native-exit-app": "^1.1.0",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-progress": "^5.0.0",
    "react-native-ratings": "^8.1.0",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-share": "^7.6.6",
    "react-native-svg": "13.4.0",
    "react-native-toast-message": "^2.1.5",
    "react-native-web": "~0.18.11",
    "react-redux": "^8.0.2",
    "redux-persist": "^6.0.0",
    "redux-persist-filesystem-storage": "^4.1.0"
  },
  "devDependencies": {
