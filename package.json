    "@babel/eslint-parser": "^7.18.2",
    "@babel/preset-env": "^7.18.6",
    "@react-native/metro-config": "^0.74.0",
    "@testing-library/dom": "^8.16.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react-native": "^12.3.2",
