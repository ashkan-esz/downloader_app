    "babel-jest": "^28.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.2.1",
    "jest-expo": "^48.0.0",
    "react-native-bundle-visualizer": "^3.1.1",
    "react-native-debugger": "^1.1.0",
    "sharp-cli": "^1.15.0"
  },
  "name": "downloader_app",
  "version": "1.0.0"
}
