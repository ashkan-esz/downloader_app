{
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "expo_start": "expo start",
    "rc-start": "react-native start --reset-cache",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "build-debug-android": "cd android && ./gradlew assembleDebug",
    "installDebug-android": "cd android && ./gradlew installDebug",
    "build-android": "cd android && ./gradlew assembleRelease",
    "installRelease-android": "cd android && ./gradlew installRelease",
    "bundle-android": "cd android && ./gradlew bundleRelease",
    "expo-build_android_apk": "expo build:android -t apk ",
    "expo-build_android_bundle": "expo build:android -t app-bundle ",
    "expo-publish": "expo publish",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean-start": "npm run clean && npm run rc-start",
    "test": "jest --verbose --coverage",
    "test:update": "jest --verbose --coverage --updateSnapshot",
    "test:watch": "jest --verbose --watch",
    "coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html",
    "bundle-visualizer": "react-native-bundle-visualizer --platform android"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./jest-setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "timers": "fake",
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:react-hooks/recommended"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.0",
    "@react-native-community/netinfo": "6.0.0",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "axios": "^0.23.0",
    "check-password-strength": "^2.0.3",
    "expo": "^42.0.0",
    "expo-app-loading": "1.1.2",
    "expo-asset": "~8.3.3",
    "expo-av": "~9.2.3",
    "expo-blur": "~9.0.3",
    "expo-clipboard": "~1.1.0",
    "expo-constants": "~11.0.1",
    "expo-font": "~9.2.1",
    "expo-linear-gradient": "~9.2.0",
    "expo-media-library": "~12.1.2",
    "expo-splash-screen": "~0.11.2",
    "expo-status-bar": "~1.0.4",
    "expo-updates": "~0.8.1",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "^7.21.0",
    "react-native": "~0.63.4",
    "react-native-dotenv": "^3.3.0",
    "react-native-elements": "^3.4.2",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-progress": "^5.0.0",
    "react-native-ratings": "^8.1.0",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "^3.10.0",
    "react-native-share": "^7.2.0",
    "react-native-svg": "12.1.1",
    "react-native-toast-message": "^1.6.0",
    "react-native-unimodules": "0.14.10",
    "react-native-web": "~0.13.12",
    "react-query": "^3.12.0"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@babel/preset-env": "^7.12.1",
    "@testing-library/dom": "^7.28.1",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react-native": "^7.1.0",
    "@testing-library/user-event": "^12.2.2",
    "@welldone-software/why-did-you-render": "^6.2.1",
    "babel-jest": "^25.2.6",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "~25.2.6",
    "jest-expo": "^42.0.0",
    "react-native-bundle-visualizer": "^3.0.0",
    "react-native-debugger": "^1.1.0",
    "react-test-renderer": "16.14.0",
    "sharp-cli": "^1.14.1"
  },
  "name": "downloader_app",
  "version": "1.0.0"
}
